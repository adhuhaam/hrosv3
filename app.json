{
  "expo": {
    "name": "HRoS",
    "slug": "hros",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "v3",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rcc.hrosv3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#F8F9FC"
      },
      "package": "com.rcc.hrosv3"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#F8F9FC"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "46ae5b5b-95f9-41d2-8266-28c9544e1108"
      }
    }
  }
}